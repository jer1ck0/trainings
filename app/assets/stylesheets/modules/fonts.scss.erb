@font-face {
  font-family: 'Open Sans';
  src:url(<%= asset_path('OpenSans-Regular.eot') %>);
  src:url(<%= asset_path('OpenSans-Regular.eot?#iefix') %>) format('embedded-opentype'),
    url(<%= asset_path('OpenSans-Regular.woff') %>) format('woff'),
    url(<%= asset_path('OpenSans-Regular.ttf') %>) format('truetype');
  font-weight: normal;
}
@font-face {
  font-family: 'Open Sans Light';
  src:url(<%= asset_path('OpenSans-Light.eot') %>);
  src:url(<%= asset_path('OpenSans-Light.eot?#iefix') %>) format('embedded-opentype'),
    url(<%= asset_path('OpenSans-Light.woff') %>) format('woff'),
    url(<%= asset_path('OpenSans-Light.ttf') %>) format('truetype');
  font-weight: normal;
}
